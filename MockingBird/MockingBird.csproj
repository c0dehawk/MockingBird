<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>MockingBird</PackageId>
    <PackageVersion>1.0.0-dev</PackageVersion>
    <Authors>Jason Thomas</Authors>
    <Description>A powerful C# library to mock data for any object or record.</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageTags>Data;Mock;Fake;Stub;Testing;Development;C#</PackageTags>  
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

</Project>
